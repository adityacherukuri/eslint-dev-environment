{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"no-console": "off",
		"quotes": ["error", "double"],
		"semi": [2, "always"],
		"allowForLoopAfterthoughts": true,
		"prettier/prettier": ["error"]
	}
}
